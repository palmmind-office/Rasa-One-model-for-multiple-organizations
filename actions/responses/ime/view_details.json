"agent_view_details":
{
    "type": "quick_reply",
    "title": "Please choose an option to view your details.",
    "data": [
    {
        "title": "Business Commission",
        "payload": "/view_details{\"category\":\"business\"}"
    },
    {
        "title": "Issued Policies",
        "payload": "/view_details{\"category\":\"issued policies\",\"user_type\":\"agent\"}"
    },
    {
        "title": "Downline Business",
        "payload": "/view_details{\"category\":\"downline\"}"
    }   
    ]
},
