{
    "agency_business":
{
        "type": "formMessageSection",
        "exitMSG": "Oops !! you quitted midway during viewing of policy commissions.",
        "form": {
            "elements": [
                {
                    "order": "text",
                    "title": "Your Last Name please?",
                    "type": "textbox",
                    "placeholder": "Please Enter Your Last Name",
                    "utterances": [
                        {
                            "message": "Please authenticate yourself by answering some questions to view commissions on policies"
                        }
                    ],
                    "validation": {
                        "type": "name",
                        "error": "Please enter valid Last Name."
                    },
                    "label": "LastName"
                },
                {
                    "order": "number",
                    "title": "Your Agent Code please ?",
                    "type": "textbox",
                    "placeholder": "Enter Agent Code",
                    "validation": {
                        "type": "AgentCode",
                        "error": "Invalid!! Provide 8 digit Agent Code."
                    },
                    "label": "agentCode"
                },
                {
                    "order": "text",
                    "title": "Please specify Date Of Birth",
                    "placeholder": "Enter DOB Here",
                    "type": "date",
                    "label": "DateOfBirth"
                },
                {
                    "order": "cancle",
                    "title": "Please confirm and click Submit button to proceed further and Cancel to quit.",
                    "type": "submitbutton",
                    "placeholder": "Enter your email",
                    "button": [
                        {
                            "submit": "Submit",
                            "type": "submit"
                        },
                        {
                            "submit": "Cancel",
                            "type": "cancel"
                        }
                    ]
                }
            ]
        },
        "post": "/rest/v1/ime/agentCommission"
},

"agency_downline":
{
    "type": "formMessageSection",
    "exitMSG": "Oops !! you quitted midway during requesting agent downline business.",
    "form": {
        "elements": [
            {
                "order": "text",
                "title": "please kindly provide your Last Name?",
                "type": "textbox",
                "placeholder": "Your Last Name",
                "utterances": [
                    {
                        "message": "Please authenticate yourself by answering some questions to see your agent downline business."
                    }
                ],
                "validation": {
                    "type": "name",
                    "error": "Please enter valid Last Name."
                },
                "label": "LastName"
            },
            {
                "order": "number",
                "title": "Enter Agent Code ?",
                "type": "textbox",
                "placeholder": "Enter Agent Code",
                "validation": {
                    "type": "AgentCode",
                    "error": "Invalid!! Provide 8 digit Agent Code"
                },
                "label": "agentCode"
            },
            {
                "order": "text",
                "title": "Please choose Business From Date",
                "placeholder": "Please Enter from date",
                "type": "FromDate",
                "label": "FromDate"
            },
            {
                "order": "text",
                "title": "Please choose Business Upto Date",
                "placeholder": "Please Enter To Date",
                "type": "ToDate",
                "label": "ToDate"
            },
            {
                "order": "number",
                "title": "Enter Downline Number ?",
                "type": "textbox",
                "placeholder": "Enter Downline",
                "validation": {
                    "type": "Downline",
                    "error": "Invalid!! Downline Number Should Be Required"
                },
                "label": "Downline"
            },
            {
                "order": "text",
                "title": "Please choose your date of birth.",
                "placeholder": "Please Enter birthdate",
                "type": "date",
                "label": "DateOfBirth"
            },
            {
                "order": "cancle",
                "title": " Do you want to submit your information ?",
                "type": "submitbutton",
                "placeholder": "Enter your email",
                "button": [
                    {
                        "submit": "Submit",
                        "type": "submit"
                    },
                    {
                        "submit": "Cancel",
                        "type": "cancel"
                    }
                ]
            }
        ]
    },
    "post": "/rest/v1/ime/agentDownlineBusiness"
},
