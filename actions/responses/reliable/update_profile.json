{
  "address": {
        "type": "formMessageSection",
        "exitMSG": "Oops !! you quitted midway. Please try again",
        "form": {
          "elements": [
            {
              "order": "number",
              "utterances": [
                {
                  "message": "Please provide following details to update your Address."
                }
              ],
              "title": "Enter your Policy Number ?",
              "type": "textbox",
              "placeholder": "Enter Policy Number",
              "validation": {
                "type": "PolicyNumber",
                "error": "Invalid!! Provide 12 digit PolicyNumber"
              },
              "label": "policyNo"
            },
            {
              "order": "text",
              "utterances": [
                {
                  "message": "Your age will help us keep you in good Services. "
                }
              ],
              "title": "Please choose your date of birth.",
              "placeholder": "Please Enter birthdate",
              "type": "date",
              "label": "dob"
            },
            {
              "order": "number",
              "title": "Provide your mobile number",
              "type": "textbox",
              "placeholder": "Enter your Mobile Number",
              "validation": {
                "type": "mobile",
                "error": "Invalid!! Provide 10 digit MobileNumber"
              }
            },
            {
              "order": "text",
              "type": "textbox",
              "title": "Please provide your new Address",
              "placeholder": "Enter new address",
              "validation": {
                "type": "requested_value",
                "error": "Address should not be empty"
              },
              "label": "requestedValue"
            },
            {
              "order": "cancle",
              "title": " Do you want to submit your information ?",
              "type": "submitbutton",
              "placeholder": "Enter your email",
              "button": [
                {
                  "submit": "Submit",
                  "type": "submit"
                },
                {
                  "submit": "Cancel",
                  "type": "cancle"
                }
              ]
            }
          ]
        },
        "post": "rest/v1/reliable/policy?type=AddressChangeRequest"
      }
